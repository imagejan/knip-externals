<!-- net.imagej bundles -->
<bundlegroup name="net.imagej">
	<bundle name="ij" version="1.49">
		<artifacts>
			<artifact> 
				<id>ij</id>
				<group>net.imagej</group>
				<version>${imagej1.version}</version>
			</artifact>
		</artifacts>
		<export>ij.*</export>
	</bundle>
	
	<bundle name="imagej-common" version="${imagej-common.version}">
		<artifacts>
			<artifact> 
				<id>imagej-common</id>
				<group>net.imagej</group>
				<version>${imagej-common.version}</version>
			</artifact>
		</artifacts>
		<dependencies>
			<bundleref name="org.scijava:scijava-common" version="${scijava-common.version}"/>
			<bundleref name="net.imglib:imglib2" version="${imglib2.version}"/>
			<bundleref name="net.imglib:imglib2-roi" version="${imglib2-roi-osgi.version}"/>
		</dependencies>
		<export>net.imagej.*, net.imglib2.meta.*</export>
	</bundle>
</bundlegroup>