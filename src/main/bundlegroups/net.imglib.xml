<!-- net.imglib bundles -->
<bundlegroup name="net.imglib">
	<bundle name="imglib2" version="1.0.0">
		<artifacts>
			<artifact>
				<id>imglib2</id>
				<group>net.imglib2</group>
				<version>${imglib2.version}</version>
			</artifact>
		</artifacts>
		<export>net.imglib2.*</export>
		<dependencies>
			<bundleref name="net.imglib:imglib2" version="${imglib2.version}"/>
			<bundleref name="net.imglib:imglib2-roi" version="${imglib2-roi-osgi.version}"/>
			<bundleref name="net.imglib:imglib2-algorithm" version="${imglib2-algorithm.version}"/>
			<bundleref name="net.imglib:imglib2-algorithm-fft" version="${imglib2-algorithm-fft.version}"/>
			<bundleref name="net.imagej:imagej-common" version="${imagej-common.version}"/>
			<bundleref name="net.scijava:scijava-common" version="${scijava-common.version}"/>
			<bundleref name="net.imglib:imglib2-algorithm-gpl" version="${imglib2-algorithm-gpl.version}"/>
			<bundleref name="net.imglib:imglib2-realtransform" version="${imglib2-realtransform-osgi.version}"/>
			<bundleref name="net.imglib:imglib2-ij" version="${imglib2-ij-osgi.version}"/>
		</dependencies>
	</bundle>
</bundlegroup>