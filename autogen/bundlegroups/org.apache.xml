<!-- trove4j bundle -->
<bundlegroup name="org.apache">
	<bundle name="commons-collections" version="${commons-collections.version}">
		<artifacts>
			<artifact>
				<group>commons-collections</group>
				<id>commons-collections</id>
				<version>${commons-collections.version}</version>
			</artifact>
		</artifacts>
		<export>*</export>
	</bundle>
	<bundle name="math3" version="${commons-math.version}">
		<artifacts>
			<artifact>
				<group>org.apache.commons</group>
				<id>math3</id>
				<version>${commons-math.version}</version>
			</artifact>
		</artifacts>
		<export>*</export>
	</bundle>
</bundlegroup>
