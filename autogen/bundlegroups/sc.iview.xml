<!-- args4j bundle -->
<bundlegroup name="sc.iview">
	<bundle name="sciview" version="${sciview.version}">
		<artifacts>
			<artifact>
				<group>sc.iview</group>
				<id>sciview</id>
				<version>${sciview.version}</version>
			</artifact>
		</artifacts>
		<export>sc.iview.*</export>
