<!-- miglayout bundle -->
<bundlegroup name="com.miglayout">
	<bundle name="miglayout" version="${miglayout.version}">
		<artifacts>
			<artifact>
				<group>com.miglayout</group>
				<id>miglayout</id>
				<version>${miglayout.version}</version>
			</artifact>
		</artifacts>
		<export>net.miginfocom.layout.*</export>
	</bundle>
	<bundle name="miglayout-swing" version="${miglayout-swing.version}">
		<artifacts>
			<artifact>
				<group>com.miglayout</group>
				<id>miglayout-swing</id>
				<version>${miglayout-swing.version}</version>
			</artifact>
		</artifacts>
		<export>net.miginfocom.swing.*</export>
	</bundle>

</bundlegroup>
