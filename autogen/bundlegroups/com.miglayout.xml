<!-- miglayout bundle -->
<bundlegroup name="com.miglayout">
	<bundle name="miglayout" version="${miglayout.version}">
		<artifacts>
			<artifact>
				<group>com.miglayout</group>
				<id>miglayout</id>
				<version>${miglayout.version}</version>
				<classifier>swing</classifier>
			</artifact>
		</artifacts>
		<export>net.miginfocom.layout.*,net.miginfocom.swing.*</export>
	</bundle>
</bundlegroup>
