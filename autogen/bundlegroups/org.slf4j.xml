<!-- trove4j bundle -->
<bundlegroup name="org.slf4j">
	<bundle name="slf4j-api" version="${slf4j-api.version}">
		<artifacts>
			<artifact>
				<group>org.slf4j</group>
				<id>slf4j-api</id>
				<version>${slf4j-api.version}</version>
			</artifact>
		</artifacts>
		<export>*</export>
	</bundle>
	<bundle name="slf4j-log4j12" version="${slf4j-log4j12.version}">
		<artifacts>
			<artifact>
				<group>org.slf4j</group>
				<id>slf4j-log4j12</id>
				<version>${slf4j-log4j12.version}</version>
			</artifact>
		</artifacts>
		<export>*</export>
	</bundle>
</bundlegroup>
